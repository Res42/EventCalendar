{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/app/startup",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}/",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy",
				"--harmony"
			],
			"env": {
				"NODE_ENV": "development",
				"PORT": "3000"
			},
			"sourceMaps": true,
			"outFiles": [
				"${workspaceRoot}/app"
			]
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach",
			"port": 5858
		}
	]
}